		{
			"name": "python-plugwise",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/services/python-plugwise/plugwise_circle.py",
			"console": "internalConsole",
		},
		{
			"name": "plugwise2pi",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/services/plugwise-2-py/Plugwise-2.py",
			"console": "internalConsole",
		},
		{
			"name": "plugwiseconnect",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/services/plugwiseconnect/__main__.py",
			"cwd": "${workspaceFolder}/services/plugwiseconnect/",
			"args": [
				"--config",
				"./config.json"
			],
			"pathMappings": [],
			"console": "internalConsole",
			"internalConsoleOptions": "openOnSessionStart",
		}
	]
